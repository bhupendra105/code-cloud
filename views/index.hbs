  <body class="scrollbar scrollbar-near-moon thin">
    <div class="force-overflow"></div>
    <a href="" id="download_link" style="display:none;">Download</a>
    <audio src="./message.mp3" id="tone" style="display:none;"></audio>
    <nav class="navbar navbar-expand-lg navbar-light bg-light">
      <div class="container">
        <h3>bhupendra code sharing</h3>
      </div>
      <div class="themechange m-2 mt-0 mb-0">
        <button
          class="btn btn-success rounded-pill"
          data-theme="light"
          onclick="chmode(this)"
        >Dark Mode</button>
      </div>
    </nav>
    <div
      class="container shadow-4 rounded border border-primary mt-3 pb-3 mb-5"
    >
      <!-- Tabs navs -->
      <ul class="nav nav-tabs nav-fill mb-3" id="ex1" role="tablist">
        <li class="nav-item" role="presentation">
          <a
            class="nav-link active"
            id="ex2-tab-1"
            data-mdb-toggle="tab"
            href="#ex2-tabs-1"
            role="tab"
            aria-controls="ex2-tabs-1"
            aria-selected="true"
          >Text</a>
        </li>
        <li class="nav-item" role="presentation">
          <a
            class="nav-link"
            id="ex2-tab-2"
            data-mdb-toggle="tab"
            href="#ex2-tabs-2"
            role="tab"
            aria-controls="ex2-tabs-2"
            aria-selected="false"
          >Code</a>
        </li>
        <li class="nav-item" role="presentation">
          <a
            class="nav-link"
            id="ex2-tab-3"
            data-mdb-toggle="tab"
            href="#ex2-tabs-3"
            role="tab"
            aria-controls="ex2-tabs-3"
            aria-selected="false"
          >File</a>
        </li>
        <li class="nav-item" role="presentation">
          <a
            class="nav-link"
            id="ex2-tab-4"
            data-mdb-toggle="tab"
            href="#ex2-tabs-4"
            role="tab"
            aria-controls="ex2-tabs-4"
            aria-selected="false"
          >Screen</a>
        </li>
      </ul>
      <!-- Tabs navs -->

      <!-- Tabs content -->
      <div class="tab-content" id="ex2-content">
        <div
          class="tab-pane fade show active"
          id="ex2-tabs-1"
          role="tabpanel"
          aria-labelledby="ex2-tab-1"
        >
          <textarea
            id="text"
            cols="30"
            rows="10"
            class="form-control"
            placeholder="Paste or Type Text Content Here!"
          ></textarea>
          <div class="d-flex justify-content-around" id="replacetext">
            <button
              class="text-send btn btn-primary btn-lg mt-2 rounded-pill"
              id="textclear"
              onclick="cleartext()"
            >
              Clear
            </button>
            <button
              class="text-send btn btn-primary btn-lg mt-2 rounded-pill"
              id="textsend"
            >
              Send
            </button>
            <button
              class="text-save btn btn-primary btn-lg mt-2 rounded-pill"
              id="textsave"
              onclick="download('text')"
            >
              Save
            </button>
          </div>
        </div>
        <div
          class="tab-pane fade"
          id="ex2-tabs-2"
          role="tabpanel"
          aria-labelledby="ex2-tab-2"
        >
          <div class="d-flex justify-content-center">
            <div class="input-group w-100">
              <input
                id="filepath"
                type="text"
                class="form-control"
                placeholder="Filename.js"
                aria-label="Example input"
                aria-describedby="button-addon1"
              />
              <button
                class="btn btn-primary"
                type="button"
                id="filepathbtn"
                data-mdb-ripple-color="dark"
              >
                Set
              </button>
            </div>
          </div>
          <div id="editor" class="codemirror form-control">
          </div>
          <div class="d-flex justify-content-around">
            <button
              class="text-send btn btn-primary btn-lg mt-2 rounded-pill"
              id="codeclear"
              onclick="clearcode()"
            >
              Clear
            </button>
            <button
              class="code-send btn btn-primary btn-lg mt-2 rounded-pill"
              id="codesend"
            >
              Send
            </button>
            <button
              class="code-save btn btn-primary btn-lg mt-2 rounded-pill"
              id="codesave"
              onclick="download('code')"
            >
              Save
            </button>
          </div>
          <!-- tab 2 -->
        </div>
        <div
          class="tab-pane fade"
          id="ex2-tabs-3"
          role="tabpanel"
          aria-labelledby="ex2-tab-3"
        >
          <div class="dragbody">
            <div class="drag-area">
              <div class="icon"><i class="fas fa-cloud-upload-alt"></i></div>
              <header>Drag & Drop to Upload File</header>
              <span>OR</span>
              <button>Browse File</button>
              <input id="file" type="file" hidden />
            </div>
          </div>
          <div class="d-flex justify-content-around">
            <button
              class="file-send btn btn-primary btn-lg mt-2 rounded-pill"
              id="filesend"
              onclick="showFile()"
            >
              Send
            </button>
          </div>
        </div>
        {{! for screen sharing }}
        <div
          class="tab-pane fade"
          id="ex2-tabs-4"
          role="tabpanel"
          aria-labelledby="ex2-tab-3"
        >
          <video src="" id="stream" class="form-control"></video>
          <button
            class="btn btn-primary mt-2 rounded-pill btn-lg"
            onclick="startSharing()"
          >Start Sharing</button>
          <button
            class="btn btn-primary mt-2 rounded-pill btn-lg"
            onclick="stopSharing()"
          >Stop Sharing</button>
        </div>
      </div>
      <hr />
      <div class="settings d-flex justify-content-evenly">
        <button class="btn btn-primary mt-2 btn-lg m-0 secret" id="setsecret">
          Set Secret
        </button>
        <button
          class="btn btn-primary mt-2 btn-lg m-0 delsecret"
          id="delsecret"
        >
          Delete Secret
        </button>
      </div>
    </div>

    <div class="container mb-5">
      <div class="accordion w-100 acd" id="basicAccordion">
      </div>
    </div>
